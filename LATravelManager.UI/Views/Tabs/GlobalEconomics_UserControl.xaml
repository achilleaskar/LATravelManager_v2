<UserControl x:Class="LATravelManager.UI.Views.Tabs.GlobalEconomics_UserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:LATravelManager.UI.Views.Tabs"
             xmlns:viewmodels="clr-namespace:LATravelManager.UI.ViewModel.Tabs.TabViewmodels"
             d:DataContext="{d:DesignInstance viewmodels:GlobalEconomics_ViewModel}"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="Ταμείο">
                <local:EconomicData_UserControl DataContext="{Binding EconomicData_ViewModel}" />
            </TabItem>
            <TabItem Header="Έσοδα">
                <local:Incomes_UserControl DataContext="{Binding Incomes_ViewModel}" />
            </TabItem>
            <TabItem Header="Έξοδα">
            </TabItem>
            <TabItem Header="Κέρδη">
                <local:TotalEarns_UserControl DataContext="{Binding TotalEarns_ViewModel}" />
            </TabItem>
            <TabItem Header="Οφειλέτες">
            <local:Remaining_UserControl DataContext="{Binding Remaining_ViewModel}" />
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>